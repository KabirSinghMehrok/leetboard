{
  "manifest_version": 3,
  "name": "Leetboard",
  "version": "1.0",
  "description": "Follow your friends on a Leetcode leaderboard",
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}
